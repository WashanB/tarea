Proceso cordobaAdolares
	Definir dolares, tasaCambio, cordobas como real;
	
	Escribir "Ingrese la cantidad de dolares:" ;
	leer dolares;
	
	Escribir "Ingrese la tasa de cambio de dolares a cordobas";
	leer tasaCambio;
	
	cordobas <- dolares*tasaCambio;
	
	Escribir "la cantidad de", dolares, "equivale a", cordobas;
FinProceso
